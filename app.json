{
  "expo": {
    "name": "Jahanuri-app",
    "slug": "app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "scheme": "jahanuri.plusbeauxjours",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jahanuri.plusbeauxjours",
      "usesAppleSignIn": true
    },
    "android": {
      "versionCode": 1,
      "package": "jahanuri.plusbeauxjours",
      "permissions": ["ACCESS_NOTIFICATION_POLICY"]
    },
    "notification": {
      "icon": "./assets/notification.png"
    }
  }
}